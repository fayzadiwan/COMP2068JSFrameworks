{{!< layout}}
<h2>Projects</h2>
<div class="row">
  {{#each projects}}
    <div class="col-md-6 mb-3">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
          <p class="card-text">{{this.description}}</p>
          <a href="{{this.link}}" class="btn btn-sm btn-outline-primary">View</a>
        </div>
      </div>
    </div>
  {{/each}}
</div>
